{
  "kind": "collectionType",
  "collectionName": "players",
  "info": {
    "singularName": "player",
    "pluralName": "players",
    "displayName": "Player",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "Player",
        "Host",
        "Mentor",
        "Founder"
      ]
    },
    "company": {
      "type": "string"
    },
    "tagline": {
      "type": "string"
    },
    "bio": {
      "type": "richtext"
    },
    "country": {
      "type": "customField",
      "customField": "plugin::country-select.country"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "socialNetworks": {
      "type": "component",
      "repeatable": true,
      "component": "contact.social-network"
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "inversedBy": "players"
    }
  }
}
