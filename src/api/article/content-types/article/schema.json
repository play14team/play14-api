{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::player.player"
    },
    "cannonical": {
      "type": "string"
    },
    "summary": {
      "type": "text"
    },
    "defaultImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "richtext"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Announcement",
        "Article",
        "Event",
        "Interview",
        "Meetup"
      ]
    },
    "tags": {
      "type": "component",
      "repeatable": true,
      "component": "shared.list"
    }
  }
}
